(() => {
  if (
    document.cookie.split(';').some((item) => item.trim().indexOf('ckns_id=') > -1)
  ) {
    return;
  }

  
    function localStorageIsAvailable() {
      try {
        const storage = window && window.localStorage;
        if(typeof storage.getItem === 'function') {
          return true
        }
        return false
      } catch (error) {
        return false
      }
    }

    if(localStorageIsAvailable()) {
      var dismissCount = Number(window.localStorage.getItem('sign-in-banner-sport-homepage-dismiss-count'));

      if (dismissCount >= 4) {
        return;
      }

      var lastDismissedDate = Number(window.localStorage.getItem('sign-in-banner-sport-homepage-last-dismissed-date'));
      var currentTime = Date.now();
      if (currentTime - lastDismissedDate < 10 * 86400000) {
        return;
      }
    }

  document.querySelector('html').classList.add('display-sign-in-banner');
})()
